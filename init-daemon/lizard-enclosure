
export JVM_ARGS=-Djava.library.path=/usr/lib/jni

case $1 in
start)
        /opt/websphere/wlp/bin/server start
        ;;
stop)
        /opt/websphere/wlp/bin/server stop
        ;;
restart)
        /opt/websphere/wlp/bin/server stop
        /opt/websphere/wlp/bin/server start
        ;;
esac
exit 0
